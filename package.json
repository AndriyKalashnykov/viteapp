{
  "name": "viteapp",
  "private": true,
  "version": "v0.0.1",
  "type": "module",
  "author": "AndriyKalashnykov(AndriyKalashnykov@gmail.com)",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --ignore-path .gitignore --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:diff": "prettier -l 'src/**/*.{ts,tsx,js,jsx}'",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint . --ext .ts,.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --ignore-path .gitignore --write 'src/**/*.{ts,tsx,js,jsx}'"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "dependencies": {
    "env-var": "^7.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@types/node": "22.18.8",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "5.0.4",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "simple-git-hooks": "2.13.1",
    "terser": "5.44.0",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "web-vitals": "5.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "overrides": {
      "svgo": "npm:@kingyue/svgo@^2.9.0"
    },
    "ignoredBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ]
  }
}
